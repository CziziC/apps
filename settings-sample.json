{
  //公开节点，即前后端都需要的节点
  "public": {
    "webservices": {
      "account": {
        "status": "active",
        "url": "https://setup.steedos.com:443"
      },
      "admin": {
        "status": "active",
        "url": "https://adminws.steedos.com:443"
      },
      "attachments": {
        "status": "active",
        "url": "https://attachmentsws.steedos.com:443"
      },
      "chat": {
        "status": "active",
        "url": "https://chatws.steedos.com:443"
      },
      "contacts": {
        "status": "active",
        "url": "https://contactsws.steedos.com:443"
      },
      "keyvalue": {
        "status": "active",
        "url": "https://keyvaluews.steedos.com:443"
      },
      "push": {
        "status": "active",
        "url": "https://pushws.steedos.com:443"
      },
      "s3": {
        "status": "active",
        "url": "https://s3ws.steedos.com"
      },
      "ubiquity": {
        "status": "active",
        "url": "https://ubiquityws.steedos.com:443"
      },
      "uuflow": {
        "status": "active",
        "url": "https://uuflowws.steedos.com:443"
      },
      "www": {
        "status": "active",
        "url": "https://www.steedos.com:443"
      }
    },
    "cfs": {
      "store": "OSS"
    },
    //系统设置相关
    "admin": {
      "disableAddSpace": false //是否禁止创建新工作区
    },
    //账户相关
    "accounts": {
      "disableAccountRegistration": false, //是否禁止注册新用户
      "UTF8_Names_Validation": "[A-Za-z0-9-_.\\u00C0-\\u017F\\u4e00-\\u9fa5]+"
    }
  },
  "push": {
    "aliyun": {
      "appKey": "xxxxxxx",
      "accessKeyId": "xxxxxxx",
      "secretAccessKey": "xxxxxxx",
      "endpoint": "http://cloudpush.aliyuncs.com",
      "apiVersion": "2015-08-27"
    },
    "apn": {
      "keyData": "xxxxxxx",
      "certData": "xxxxxxx"
    },
    "gcm": {
      "projectNumber": "xxxxxx",
      "apiKey": "xxxxxxx"
    }

  },
  "cfs": {
    "aliyun": {
      "region": "xxxxxx",
      "internal": false,
      "bucket": "xxxxxx",
      "folder": "xxxxxx",
      "accessKeyId": "xxxxxx",
      "secretAccessKey": "xxxxxx"
    },
    "aws": {
      "region": "xxxxxx",
      "bucket": "xxxxxx",
      "folder": "xxxxxx",
      "accessKeyId": "xxxxxx",
      "secretAccessKey": "xxxxxx"
    }
  },
  "cron": {
    "bqq": "0 * * * * *",
    "dingtalk": "0 * * * * *",
    "statistics": "0 0 23 * * *",
    "billing": "0 * * * * *",
    "mailqueue_interval": 1000,
    "push_interval": 1000
  },
  "email": {
    "from": "Steedos <xxx@xxxxxx>"
  },
  "imo": {
    "appcid": "xxx",
    "appuid": "xxx",
    "sync_app_key": "xxx",
    "push_app_key": "xxx"
  },
  //日志相关
  "logger": {
    "enabled": true, //是否启用日志
    "showPackage": true, //是否显示包前缀
    "showFileAndLine": true, //是否显示文件及行号
    "logLevel": 2, //日志级别，0为只输出error日志，2为输出所有级别日志
    "viewLimit": 1000 //日志最大记录数
  }
}